{
  "name": "bookshop",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@sap/cds": "^9",
    "cds-softdelete-plugin": "file:../../.",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@cap-js/cds-test": "^0",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "start": "cds-serve",
    "test": "node --test",
    "update": "npm install && npm install --install-links",
    "watch-books": "cds watch --open ns.books/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "private": true,
  "cds": {
    "log": {
      "levels": {
        "soft-delete": "debug",
        "cds": "silent"
      }
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/books"
  ]
}
